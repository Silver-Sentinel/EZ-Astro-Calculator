[package]
name = "astro-calc"
version = "0.1.0"
edition = "2021"

[dependencies]
tauri = { version = "2", features = [] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
chrono = "0.4"
reqwest = { version = "0.12", features = ["json"] }
urlencoding = "2.1"
tokio = { version = "1", features = ["full"] }
regex = "1.10"

[build-dependencies]
tauri-build = { version = "2", features = [] }
cc = "1.0"

[lib]
name = "astro_calc"
crate-type = ["cdylib", "rlib"]

[[bin]]
name = "astro-calc"
path = "src/main.rs"
